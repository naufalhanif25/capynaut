  "description": "Capynaut is a simple library for handling keyboard and click shortcuts on a specific DOM element or the global document. Capynaut can be used in JavaScript and Typescript.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.js",
  "repository": {
    "url": "https://github.com/naufalhanif25/capynaut"
  },
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "scripts": {
    "build": "tsup",
    "test": "jest"
  },
  "keywords": [
    "shortcuts",
    "keyboard",
    "mouse",
    "events"
  ],
  "author": "naufalhanif",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.0",
